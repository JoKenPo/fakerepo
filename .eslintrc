{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "prettier",
    "eslint:recommended",
    "plugin:react/jsx-runtime",
    "plugin:import/recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "@typescript-eslint",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "react/button-has-type": "off",
    "no-console": 0,
    "no-empty": 0,
    "no-unused-vars": 1,
    "eqeqeq": 0,
    "no-plusplus": 0,
    "import/extensions": "off",
    "import/no-unresolved": 0,
    "eslintjsx-a11y/interactive-supports-focus": 0
  },
  "overrides": [
    {
      "files": [
        "commons/*.js",
        "components/*.js"
      ]
    }
  ]
}
